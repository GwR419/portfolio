<?xml version="1.0" encoding="UTF-8"?>
<root>
    <heading date="1858-08-23">August 23.</heading>

    <condition type="weather">
        <weather type="heavy-rain"/>
        <location place="newfoundland"/>
        <time when="12:00 P.M.-6:00 A.M."/>
    </condition>

    <sender-reciever sender="newfoundland" reciver="valentia"/>
    <message>“Your currents very irregular. Repeat.”</message>

    <sender-reciever sender="valentia" reciever="newfoundland"/>
    <message>
        <condition type="weak-signal">“Signals weak.</condition>
        <question>Will you take message from <person who="Sir Robert Walter Carden">Lord
                Mayor?</person>”</question>
    </message>

    <sender-reciever sender="newfoundland" reciever="valentia"/>
    <message>
        <answer type="yes">“Yes;</answer>
        <request>send little faster and better.”</request>
    </message>

    <sender-reciever sender="valentia" reciever="newfoundland"/>
    <message>“Give ‘understand ‘every twenty words.” </message>
    <sender-reciever sender="newfoundland" reciever="valentia"/>
    <message>“All right. Send much faster.”</message>
    <message>“Understand, faster.”</message>
    <issue>
        <time when="almost-entire-day">
            <verb>Testing</verb>
            <location where="valentia"/>
        </time>
    </issue>
</root>
